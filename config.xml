<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "onde/w.pw.zer0"
        version     = "0.1" >
   <name>Onde mesmo?</name>
   <description>
       Onde Mesmo? é um aplicativo em que você grava sua localização, para encontrá-la futuramente.
   </description>
   <author href="http://zer0.w.pw/" email="contato@zer0.w.pw">
       ZER0 Soluções Interativas
   </author>
   <gap:plugin name="org.apache.cordova.geolocation" version="0.3.6" />
   <preference name="permissions" value="geolocation"/>
	<gap:platform name="android" />
   <icon src="icon.png" />
   <icon src="mdpi.png" gap:platform="android" gap:density="mdpi" />
	<icon src="hdpi.png" gap:platform="android" gap:density="hdpi" />
	<icon src="xhdpi.png" gap:platform="android" gap:density="xhdpi" />
	<icon src="xxhdpi.png" gap:platform="android" gap:density="xxhdpi" />
   <access origin="*"/>
</widget>